<templateSet group="Custom Templates">
  <template name="log" value="console.log(`$item$: ${$item$}`);$END$" description="Inserts a `console.log()` instance" toReformat="true" toShortenFQNames="true">
    <variable name="item" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="css" value="css`&#10;  $END$&#10;`;" description="Creates a `css` instance from @emotion" toReformat="false" toShortenFQNames="true" deactivated="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="fx" value="display: flex;&#10;align-items: center;&#10;justify-content: center;" description="Inserts centered flex styles" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="fxcol" value="display: flex;&#10;align-items: center;&#10;justify-content: center;&#10;flex-direction: column;" description="Inserts centered flex column styles" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="tsi" value="// @ts-ignore" description="Inserts a // &quot;@ts-ignore&quot;" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="rfc" value="const $1$ = (props) =&gt; {&#10;  return (&#10;    &lt;$3$&gt;&#10;      $END$&#10;    &lt;/$3$&gt;&#10;  );&#10;};&#10;&#10;export default $1$;" description="Generates the scaffolding of React functional component" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="3" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="usee" value="React.useEffect(() =&gt; {&#10;  $end$&#10;}, [$1$]);" description="Creates a default 'useEffect' instance" toReformat="true" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="uses" value="const [$state$, set$state_$] = React.useState($init$);$end$" description="Creates a 'useState' instance" toReformat="true" toShortenFQNames="true">
    <variable name="state" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="state_" expression="capitalize(state)" defaultValue="" alwaysStopAt="true" />
    <variable name="init" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="int" value="interface $1$Props {&#10;  $END$&#10;}" description="Creates typescript &quot;interface&quot; instance" toReformat="true" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="el" value="&lt;$ELEMENT$$PROPS$&gt;&#10;  $END$&#10;&lt;/$ELEMENT$&gt;" description="Creates a JSX element" toReformat="true" toShortenFQNames="true">
    <variable name="ELEMENT" expression="" defaultValue="div" alwaysStopAt="true" />
    <variable name="PROPS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="timeout" value="setTimeout(() =&gt; $END$, $TIME$);" description="Creates a `setTimeout` instance" toReformat="true" toShortenFQNames="true">
    <variable name="TIME" expression="completeSmart()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="interval" value="setInterval(() =&gt; $END$, $TIME$);" description="Creates a `setInterval` instance" toReformat="true" toShortenFQNames="true">
    <variable name="TIME" expression="completeSmart()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>